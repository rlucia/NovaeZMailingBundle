{
    "name": "novactive/ezmailingbundle",
    "description": "Novactive eZ Mailing Bundle is an eZ Platform bundle that provides mailing features",
    "keywords": [
        "mailing",
        "newsletter",
        "ezpublish",
        "novactive",
        "ezplatform"
    ],
    "homepage": "https://github.com/Novactive/NovaeZMailingBundle",
    "type": "ezplatform-bundle",
    "authors": [
        {
            "name": "Novactive",
            "homepage": "https://www.novactive.us",
            "email": "ezmailing@novactive.com"
        },
        {
            "name": "Sebastien Morel",
            "homepage": "https://github.com/Plopix",
            "email": "morel.seb@gmail.com"
        }
    ],
    "license": [
        "MIT"
    ],
    "require": {
        "php": "^7.1",
        "nesbot/carbon": "^1.22|^2.16",
        "symfony/thanks": "^1.0",
        "ezsystems/ezpublish-kernel": "^7.1",
        "phpoffice/phpspreadsheet": "^1.1",
        "ext-json": "*"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.2",
        "phpmd/phpmd": "^2.6",
        "friendsofphp/php-cs-fixer": "^2.10",
        "fzaninotto/faker": "^1.7",
        "doctrine/doctrine-fixtures-bundle": "^3.0",
        "phpunit/phpunit": "^7.1"
    },
    "autoload": {
        "psr-4": {
            "Novactive\\Bundle\\eZMailingBundle\\": "bundle",
            "Novactive\\Bundle\\eZMailingBundle\\Tests\\": "tests/Tests"
        }
    },
    "extra": {
        "thanks": {
            "name": "ezsystems/ezplatform",
            "url": "https://github.com/ezsystems/ezplatform"
        }
    }
}
